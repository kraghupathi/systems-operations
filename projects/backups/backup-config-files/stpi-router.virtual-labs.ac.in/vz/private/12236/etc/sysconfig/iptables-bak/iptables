-A INPUT -s 192.168.0.0/16 -p tcp -m tcp --dport 22 -j ACCEPT
-A INPUT -p tcp -m tcp --dport 5666 -j ACCEPT
#Allow loopback connections
-A INPUT -i lo -j ACCEPT
#Deny everthing else
-A INPUT -j REJECT
